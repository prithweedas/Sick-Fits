{
  "name": "sick-fits-backend",
  "version": "1.0.0",
  "description": "the backend of sick fits",
  "scripts": {
    "build": "babel src -d dist --copy-files",
    "start": "node dist/index.js",
    "dev": "nodemon -e js,graphql -x babel-node --inspect src/index.js",
    "test": "jest",
    "deploy": "prisma deploy --env-file variables.env"
  },
  "dependencies": {
    "bcryptjs": "2.4.3",
    "cookie-parser": "^1.4.3",
    "graphql": "^0.13.2",
    "graphql-cli": "^2.16.7",
    "graphql-yoga": "1.16.2",
    "jsonwebtoken": "8.3.0",
    "nodemailer": "^4.6.8",
    "prisma": "1.17.1",
    "prisma-binding": "2.1.6",
    "stripe": "^6.12.1"
  },
  "devDependencies": {
    "dotenv": "6.0.0",
    "npm-run-all": "^4.1.5",
    "babel-cli": "^6.26.0",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-preset-env": "^1.7.0",
    "nodemon": "^1.18.7"
  },
  "license": "MIT",
  "repository": {
    "url": "wesbos.com"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "10.15.0"
          }
        }
      ]
    ],
    "plugins": [
      "syntax-object-rest-spread"
    ]
  }
}
